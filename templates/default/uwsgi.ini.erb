[uwsgi]
plugins = logfile,python

logto = file:/var/log/tcr-datasets.log

master = true
processes = 2
pp = /opt/tcr_datasets/current/tcr_scripts/web/datasets
chdir = /opt/tcr_datasets/current/tcr_scripts/web/datasets
wsgi-file = datasets.py
callable = app

socket = /tmp/.tcr-datasets.sock
chmod-socket = 660
uid = www-data
gid = www-data
vacuum = true

die-on-term = true
